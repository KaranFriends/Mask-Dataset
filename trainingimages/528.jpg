# importing os module 
import os 
  
# Function to rename multiple files 
def main(): 
#    i = 0
    xyz = "/home/karan/Projects/mask_git/Mask-Dataset/trainingimages/"
    for count, filename in enumerate(os.listdir(xyz)): 
        dst = str(count) + ".jpg"
        src = xyz + filename 
        dst = xyz + dst
 #       i+=1
        # rename() function will 
        # rename all the files 
        os.rename(src, dst) 
  
# Driver Code 
if __name__ == '__main__': 
      
    # Calling main() function 
    main() 
